# Telegram API Configuration
# Get these values from https://my.telegram.org
API_ID=
API_HASH=

# Telegram Session String
# Leave empty for new session, will be generated on first run
SESSION=

# Phone number for authentication
PHONE=

# Channel to monitor
CHANNEL=

# Storage path for downloaded data
STORAGE_PATH=./data

# Sync settings
SYNC_LIMIT=5

# Eitaa API Configuration
# Get your token from eitaayar.ir panel
EITAA_TOKEN=

# Eitaa channel/chat ID or username (without @)
EITAA_CHAT_ID=